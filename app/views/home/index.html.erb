                </ul>
            </div>
        </div>
        <!--end options-->
              

              
        <!--start images-->
        <div id="containerisotope" class="clear">    

          <% @mobile_pictures[0..10].each do |pic| %>
              <div class="element yellow" data-category="yellow">
